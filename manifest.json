{
	"name": "DataDog Midnight Graph",
	"version": "1.0",
	"description": "Changes the Datadog graph to be loaded from midnight 00:00",
	"permissions": ["activeTab", "declarativeContent"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2

}
